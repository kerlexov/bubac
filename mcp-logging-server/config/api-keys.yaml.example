require_auth: true
api_keys:
  # Example API key (hashed)
  # Original key: mcp_1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef
  "a1b2c3d4e5f6789012345678901234567890abcdef1234567890abcdef12345678": 
    name: "production-service"
    permissions:
      - "ingest_logs"
      - "metrics"
    rate_limit: 5000
    created_at: "2024-01-15T10:30:00Z"
    is_active: true
    
  # Admin key example
  "b2c3d4e5f6789012345678901234567890abcdef1234567890abcdef123456789":
    name: "admin-key"
    permissions:
      - "admin"
    rate_limit: 10000
    created_at: "2024-01-15T10:30:00Z"
    expires_at: "2025-01-15T10:30:00Z"
    is_active: true
    
  # Read-only key for monitoring
  "c3d4e5f6789012345678901234567890abcdef1234567890abcdef1234567890":
    name: "monitoring-readonly"
    permissions:
      - "query_logs"
      - "metrics"
    rate_limit: 1000
    created_at: "2024-01-15T10:30:00Z"
    is_active: true